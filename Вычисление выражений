A3)
#include <stdio.h>

int main()
{
    int a=0,b=0,c=0,s=0;
    scanf ("%d", &a);
    scanf ("%d", &b);
    scanf ("%d", &c);
    s = a+b+c;
    printf ("%d", a); printf("+"); printf("%d", b); printf("+");
    printf ("%d", c); printf("="); printf("%d", s);
    return 0;
}

A4)
#include <stdio.h>

int main()
{
    int a=0,b=0,c=0,s=0,p=0;
    scanf ("%d", &a);
    scanf ("%d", &b);
    scanf ("%d", &c);
    s = a+b+c;
    p = a*b*c;
    printf ("%d", a); printf("+"); printf("%d", b); printf("+");
    printf ("%d", c); printf("="); printf("%d", s); printf("\n");
    printf ("%d", a); printf("*"); printf("%d", b); printf("*");
    printf ("%d", c); printf("="); printf("%d", p);
    return 0;
}

A5)
#include <stdio.h>

int main()
{
    int a=0,b=0,c=0;
    double s = 0.0;
    scanf ("%d", &a);
    scanf ("%d", &b);
    scanf ("%d", &c);
    s = (a+b+c)/3.0;
    printf ("%f", s);
    return 0;
}


A6)
#include <stdio.h>

int main()
{
    int a=0,b=0,s=0;
    scanf ("%d", &a);
    scanf ("%d", &b);
    s = a-b;
    printf ("%d", s);
    return 0;
}

A7)
#include <stdio.h>

int main()
{
    int a=0,b=0;
    scanf ("%d", &a);
    scanf ("%d", &b);
    if (b>a){
        printf("%d", a); printf(" "); printf("%d", b);
    }
    else{
        printf("%d", b); printf(" "); printf("%d", a);
    }
    return 0;
}
