B1)
#include <stdio.h>
#include <math.h>

int main()
{
    int a=0;
    scanf("%d", &a);
    for (int i=1; i<a+1; i++){
        printf("%d", i); printf(" "); printf("%d", i*i); printf(" ");
        printf("%d", i*i*i); printf("\n");
    }
    return 0;
}

B2)
#include <stdio.h>
#include <math.h>

int main()
{
    int a=0,b=0;
    scanf("%d", &a);
    scanf("%d", &b);
    for (int i=a; i<b+1; i++){
        printf("%d", i*i); printf(" ");
    }
    return 0;
}

B3)
#include <stdio.h>
#include <math.h>

int main()
{
    int a=0,b=0,s=0;
    scanf("%d", &a);
    scanf("%d", &b);
    for (int i=a; i<b+1; i++){
        s += i*i;
    }
    printf("%d", s);
    return 0;
}

B4)
#include <stdio.h>

int main()
{
    int a=0,s=0;
    scanf("%d", &a);
    while(a > 0){
        a = a/10;
        s += 1;
    }
    if (s==3){
        printf("YES");
    }
    else{
        printf ("NO");
    }
    return 0;
}

B5)
#include <stdio.h>

int main()
{
    int a=0,s=0;
    scanf("%d", &a);
    while(a > 0){
        s += a%10;
        a = a/10;
    }
    printf("%d", s);
    return 0;
}

B6)
#include <stdio.h>

int main()
{
    int a=0,b=0,s=0;
    scanf("%d", &a);
    while(a > 0){
        b = a%10;
        a = a/10;
        if (a%10 == b){
            s = 1;
        }
    }
    if (s==1){
        printf("YES");
    }
    else{
        printf("NO");
    }
    return 0;
}
